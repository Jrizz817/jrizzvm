{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/animals.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/animals",
      "methods": ["GET","POST"],
      "dest": "/api/animals.js"
    },
    {
      "src": "/api/notify",
      "methods": ["GET","POST"],
      "dest": "/api/notify.js"
    }
  ]
}
